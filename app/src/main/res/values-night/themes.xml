<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.EminentAlerts" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <item name="colorPrimary">@color/green</item>
        <item name="colorOnPrimary">@color/mid</item>
        <item name="colorSecondary">@color/blue</item>
        <item name="colorAccent">@color/light_blue</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="Theme.PdfView.SelectedTheme" parent="@style/Theme.PdfView.Dark">
        <item name="pdfView_showToolbar">true</item>
        <item name="pdfView_disableScreenshots">false</item>
        <item name="pdfView_actionBarTint">@color/green</item>
        <item name="pdfView_backgroundColor">@color/light_blue</item>
        <item name="android:statusBarColor">@color/mid</item>
    </style>
    <style name="DialogActivityTheme" parent="Theme.AppCompat.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsFloating">true</item>
    </style>

</resources>